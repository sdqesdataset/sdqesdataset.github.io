    {
      "@type": "cr:FileObject",
      "@id": "annotations.csv",
      "name": "annotations.csv",
      "contentSize": "TODO",
      "contentUrl": "TODO",
      "encodingFormat": "text/csv",
      "sha256": "TODO"
    }
  ],
  "recordSet": [
    {
      "@type": "cr:RecordSet",
      "@id": "annotations",
      "name": "annotations",
      "description": "The list of annotations. Does not include videos.",
      "field": [
        {
          "@type": "cr:Field",
          "@id": "annotations/split",
          "name": "split",
          "description": "Split of the annotation. One of 'train' or 'val'.",
          "dataType": "sc:Text",
          "source": {
            "fileObject": {
              "@id": "annotations.csv"
            },
            "extract": {
              "column": "split"
            }
          }
        },
        {
            "@type": "cr:Field",
            "@id": "annotations/video_uid",
            "name": "video_uid",
            "description": "The unique identifier of the Ego4D video.",
            "dataType": "sc:Text",
            "source": {
              "fileObject": {
                "@id": "annotations.csv"
              },
              "extract": {
                "column": "video_uid"
              }
            }
          },
          {
            "@type": "cr:Field",
            "@id": "annotations/query",
            "name": "query",
            "description": "The query referring to the start of an event in the video.",
            "dataType": "sc:Text",
            "source": {
              "fileObject": {
                "@id": "annotations.csv"
              },
              "extract": {
                "column": "query"
              }
            }
          },
          {
            "@type": "cr:Field",
            "@id": "annotations/response",
            "name": "response",
            "description": "The expected model response when the queried event starts.",
            "dataType": "sc:Text",
            "source": {
              "fileObject": {
                "@id": "annotations.csv"
              },
              "extract": {
                "column": "response"
              }
            }
          },
          {
            "@type": "cr:Field",
            "@id": "annotations/video_start_time",
            "name": "video_start_time",
            "description": "The start timestamp of the event in the video.",
            "dataType": "sc:Float",
            "source": {
              "fileObject": {
                "@id": "annotations.csv"
              },
              "extract": {
                "column": "video_start_time"
              }
            }
          },
          {
            "@type": "cr:Field",
            "@id": "annotations/video_end_time",
            "name": "video_end_time",
            "description": "The end timestamp of the event in the video.",
            "dataType": "sc:Float",
            "source": {
              "fileObject": {
                "@id": "annotations.csv"
              },
              "extract": {
                "column": "video_end_time"
              }
            }
          },
          {
            "@type": "cr:Field",
            "@id": "annotations/video_fps",
            "name": "video_fps",
            "description": "Metadata of the video: frames per second of the video.",
            "dataType": "sc:Integer",
            "source": {
              "fileObject": {
                "@id": "annotations.csv"
              },
              "extract": {
                "column": "video_fps"
              }
            }
          },
          {
            "@type": "cr:Field",
            "@id": "annotations/video_fps",
            "name": "video_fps",
            "description": "Metadata of the video: total length of the video in frames.",
            "dataType": "sc:Integer",
            "source": {
              "fileObject": {
                "@id": "annotations.csv"
              },
              "extract": {
                "column": "video_fps"
              }
            }
          }
